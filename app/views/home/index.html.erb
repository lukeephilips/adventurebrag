<div class="row">
  <div class="col s8 offset-s2">
    <div class="card-panel intro">
      <h5>AdventureBrags is a site to brag about the adventures you go on. Login to contribute, or click an adventure to view</h5>
    </div>
    <div class="carousel carousel-slider center" data-indicators="true">
      <% Place.first(5).each do |place| %>
        <%= link_to image_tag(place.image, class: 'carousel-item'), place_path(place) %>
      <% end %>
    </div>
  </div>
</div>
